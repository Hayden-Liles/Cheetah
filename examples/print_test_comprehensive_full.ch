print("=== Basic Types ===")
print("Integer:", 42)
print("Float:", 3.14159)
print("Boolean:", True, False)
print("None:", None)
print("String:", "Hello, World!")
print()

print("=== Lists ===")
print("Empty list:", [])
print("Number list:", [1, 2, 3])
print("Mixed list:", ["a", 1, 1.1])
print()

print("=== Nested Lists ===")
print("Nested list:", [[1, 2], [3, 4]])
print("Matrix:", [[1, 2, 3], [4, 5, 6], [7, 8, 9]])
print()

print("=== Dictionaries ===")
print("Empty dictionary:", {})
print("Person dictionary:", {"name": "Alice", "age": 30, "is_student": False})
print()

print("=== Nested Dictionaries ===")
print("Nested dictionary:", {"person": {"name": "Bob", "age": 25}, "scores": [90, 85, 92]})
print()

print("=== Mixed Nesting ===")
print("Complex structure:", {"name": "Charlie", "data": [1, 2, 3], "metadata": {"created": "today", "modified": "now"}})
print()

print("=== List of Dictionaries ===")
print("Users:", [{"name": "Alice", "age": 30}, {"name": "Bob", "age": 25}, {"name": "Charlie", "age": 35}])
print()

print("=== Dictionary of Lists ===")
print("Categories:", {"fruits": ["apple", "banana", "orange"], "vegetables": ["carrot", "broccoli"], "numbers": [1, 2, 3, 4, 5]})
print()

print("=== Multiple Arguments ===")
print("Multiple values:", 1, "two", 3.0, [4, 5], {"six": 6})
print()

print("=== Modifying and Printing ===")
numbers = [1, 2, 3, 4, 5]
print("Before append:", numbers)
numbers.append(6)
print("After append:", numbers)
print()

print("=== Print Formatting ===")
name = "Alice"
age = 30
scores = [95, 87, 92]
print("Name:", name, "Age:", age, "Scores:", scores)
print()

print("=== Print with Expressions ===")
print("Sum:", 5 + 10, "Product:", 5 * 10, "List:", [1, 2] + [3, 4])
print()

print("=== String Concatenation ===")
print("Concatenated:", "Hello" + " " + "World")
print()

print("=== Boolean Expressions ===")
print("Comparisons:", 5 > 3, 5 < 3, 5 == 3, 5 != 3)
print()

print("=== End of Test ===")
