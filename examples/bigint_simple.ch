# Simple test for big integer operations

# Test with small numbers
print("Small number tests:")
print("1234567890 + 9876543210 =")
print(1234567890 + 9876543210)

# Test with larger numbers (requires BigInt)
print("\nLarge number tests:")
print("i64::MAX =")
print(9223372036854775807)
print("i64::MAX + 1 =")
print(9223372036854775807 + 1)
print("i64::MAX + i64::MAX =")
print(9223372036854775807 + 9223372036854775807)

# Test Fibonacci numbers
print("\nFibonacci numbers:")

# F(0) and F(1)
print("F(0) = 0")
print("F(1) = 1")

# F(2) = F(1) + F(0) = 1 + 0 = 1
print("F(2) = 1")

# F(3) = F(2) + F(1) = 1 + 1 = 2
print("F(3) = 2")

# F(4) = F(3) + F(2) = 2 + 1 = 3
print("F(4) = 3")

# F(5) = F(4) + F(3) = 3 + 2 = 5
