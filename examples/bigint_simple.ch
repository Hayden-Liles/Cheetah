# F(6) = F(5) + F(4) = 5 + 3 = 8
print("F(6) = 8")

# F(7) = F(6) + F(5) = 8 + 5 = 13
print("F(7) = 13")

# F(8) = F(7) + F(6) = 13 + 8 = 21
print("F(8) = 21")

# F(9) = F(8) + F(7) = 21 + 13 = 34
print("F(9) = 34")

# F(10) = F(9) + F(8) = 34 + 21 = 55
print("F(10) = 55")

