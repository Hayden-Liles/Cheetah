# Test big integer operations in Cheetah

# Test with small numbers (fits in i64)
print("Small number tests:")
print("1234567890 + 9876543210 =")
print(1234567890 + 9876543210)
print("1234567890 - 9876543210 =")
print(1234567890 - 9876543210)
print("1234567890 * 9876543210 =")
print(1234567890 * 9876543210)

# Test with larger numbers (requires BigInt)
print("\nLarge number tests:")
print("i64::MAX =")
print(9223372036854775807)
print("i64::MAX + 1 =")
print(9223372036854775807 + 1)
print("i64::MAX + i64::MAX =")
print(9223372036854775807 + 9223372036854775807)
print("i64::MAX * 2 =")
print(9223372036854775807 * 2)

# Test with even larger numbers
print("\nVery large number tests:")
print("i64::MAX * 2 =")
print(9223372036854775807 * 2)
print("(i64::MAX * 2) + (i64::MAX * 2) =")
print((9223372036854775807 * 2) + (9223372036854775807 * 2))
print("(i64::MAX * 2) * 10 =")
print((9223372036854775807 * 2) * 10)
