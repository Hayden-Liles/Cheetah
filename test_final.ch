# Test basic expressions, if expressions, and boolean operations
a = 1
b = 2
c = a + b
print(c)

# If expression
d = 10 if c > 2 else 5
print(d)

# Boolean operations
e = True and False
f = True or False
print(e)
print(f)

# Comparison operations
g = a < b
h = a == b
i = a != b
print(g)
print(h)
print(i)

# Nested expressions
j = (a + b) * (c - 1)
print(j)
