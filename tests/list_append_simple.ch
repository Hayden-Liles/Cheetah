# Simple test for list.append method

# Create a list
numbers = [1, 2, 3]

# Append a value
numbers.append(4)

# Append another value
numbers.append(5)

# Print the final list
print("Final list:", numbers)

# Create an empty list
empty = []

# Append values to the empty list
empty.append(10)
empty.append(20)

# Print the list
print("List from empty:", empty)
